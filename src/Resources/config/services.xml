<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Workshop\Plugin\WorkshopWishlist\Entity\Product\ProductExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="Workshop\Plugin\WorkshopWishlist\Entity\Wishlist\WishlistDefinition">
            <tag name="shopware.entity.definition" entity="workshop_wishlist"/>
        </service>

        <service id="Workshop\Plugin\WorkshopWishlist\Entity\Wishlist\Aggregate\WishlistProduct\WishlistProductDefinition">
            <tag name="shopware.entity.definition" entity="workshop_wishlist_product"/>
        </service>

    </services>
</container>
